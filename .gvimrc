" Open Vim HUGE
set columns=1000
set lines=1000

" Remove toolbar
set guioptions-=T

" Remove scrollbars
set guioptions-=L
set guioptions-=r

" Don't uset GUI dialogs
set guioptions+=c

" Don't uset UI chrome for tabs
set guioptions-=e

" Use Solarized
colorscheme solarized

" Dark background
set background=dark

" Font
set guifont=Menlo:h17
